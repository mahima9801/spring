<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.capg.beans.Sbu" id="sbu">
	<property name="sbuCode" value="PES-BU"/>
	<property name="sbuName" value="John"/>
	<property name="sbuHead" value="Product Engineering services"/>
	<property name="empList">
	<list>
	<ref bean="emp1"/>
	</list>
	</property>
	</bean>
	
	<bean class="com.capg.beans.Employee" id="emp1">
	<property name="employeeId" value="1001"/>
	<property name="employeeName" value="Mahima"/>
	<property name="salary" value="50000"/>
	<property name="businessUnit" value="B-SE"/>
	<property name="age" value="24"/>
	</bean>
	
	
</beans>
